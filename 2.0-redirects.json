---
layout: null
---
{%- for page in site.pages -%}
  {%- if page.redirect_from contains "guides/v2.0" -%}
    {%- assign to=page.path | replace: ".md", ".html" -%}
    {%- assign from=page.redirect_from | remove_first: "/" -%}
{
  "RoutingRules": [
    {
      "Condition": {
         "KeyPrefixEquals": "{{ from }}" 
       },
       "Redirect": {
         "HttpRedirectCode": "301",
         "ReplaceKeyPrefixWith": "{{ to }}"
       }
    }
 ]
}
  {%- endif -%}
{%- endfor -%}
